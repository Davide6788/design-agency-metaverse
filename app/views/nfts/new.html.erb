<%= simple_form_for [ @nft ] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :rented %>
  <select name="category[category]">
    <% Category.all.each do |category| %>
      <option value=<%= category.id %>><%= category.category_name %></option>
    <% end %>
  </select>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create NFT", class: "btn btn-primary" %>
<% end %>
